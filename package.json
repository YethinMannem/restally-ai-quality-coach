{
  "name": "restally",
  "version": "0.1.0",
  "description": "RestAlly â€“ an AI-assisted quality coach for REST APIs",
  "main": "dist/index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "api": "tsx samples/server.ts",
    "gen:http": "tsx src/generateHttpTests.ts samples/petstore.yaml tests/generated.http.spec.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.cjs --runInBand",
    "prep:reports": "node -e \"require('fs').mkdirSync('reports',{recursive:true})\"",
    "test:json": "npm run prep:reports && npm run gen:http && node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.cjs --runInBand --json --outputFile reports/jest.json",
    "report": "tsx src/report/makeHtmlReport.ts reports/jest.json reports/index.html",
    "test:report": "npm run test:json && npm run report",
    "mutation": "npx stryker run"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "express": "^4.19.2",
    "openapi-types": "^12.1.3",
    "supertest": "^7.0.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^9.2.0",
    "@stryker-mutator/jest-runner": "^9.2.0",
    "@stryker-mutator/typescript-checker": "^9.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4"
  }
}
